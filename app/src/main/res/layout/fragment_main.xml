                android:layout_height="wrap_content"
                android:layout_marginHorizontal="@dimen/_10sdp"
                android:layout_marginTop="@dimen/_15sdp"
                android:orientation="vertical">


                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:fontFamily="@font/ramabhadra"
                    android:text="New Menu"
                    android:textColor="@color/calHeaderT"
                    android:textSize="@dimen/_16sdp" />


                <com.google.android.material.card.MaterialCardView
                    app:cardUseCompatPadding="true"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_120sdp"
                    app:cardBackgroundColor="@color/white"
                    app:cardCornerRadius="@dimen/_10sdp"
                    app:cardMaxElevation="@dimen/_2sdp">

                    <LinearLayout
                        android:layout_gravity="center"
                        android:weightSum="4"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:orientation="horizontal"
                        android:paddingVertical="@dimen/_5sdp">

                        <LinearLayout
                            android:id="@+id/llGovidaNamalu"
                            android:layout_weight="1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:orientation="vertical">

                            <com.google.android.material.card.MaterialCardView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                app:cardCornerRadius="14dp"
                                app:cardElevation="0dp">

                                <ImageView
                                    android:layout_width="70dp"
                                    android:layout_height="70dp"
                                    android:layout_gravity="center"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_10" />

                            </com.google.android.material.card.MaterialCardView>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:fontFamily="@font/ramabhadra"
                                android:maxLines="2"
                                android:text="Govida Namalu"
                                android:textAlignment="center"
                                android:textColor="@color/black"
                                android:textSize="15sp" />
                        </LinearLayout>

                        <Space
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            />

                        <LinearLayout
                            android:id="@+id/llAliveluManga"
                            android:layout_weight="1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <com.google.android.material.card.MaterialCardView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                app:cardCornerRadius="14dp"
                                app:cardElevation="0dp">

                                <ImageView
                                    android:layout_width="70dp"
                                    android:layout_height="70dp"
                                    android:layout_gravity="center"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_11" />

                            </com.google.android.material.card.MaterialCardView>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:fontFamily="@font/ramabhadra"
                                android:maxLines="2"
                                android:text="Alivelu Manga"
                                android:textAlignment="center"

                                android:textColor="@color/black"
                                android:textSize="15sp" />
                        </LinearLayout>

                        <Space
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            />

                        <LinearLayout
                            android:id="@+id/llPushkaralu"
                            android:layout_weight="1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <com.google.android.material.card.MaterialCardView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                app:cardCornerRadius="14dp"
                                app:cardElevation="0dp">

                                <ImageView
                                    android:layout_width="70dp"
                                    android:layout_height="70dp"
                                    android:layout_gravity="center"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_12" />

                            </com.google.android.material.card.MaterialCardView>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:fontFamily="@font/ramabhadra"
                                android:maxLines="2"
                                android:text="Pushkaralu"
                                android:textAlignment="center"

                                android:textColor="@color/black"
                                android:textSize="15sp" />
                        </LinearLayout>

                        <Space
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            />

                        <LinearLayout
                            android:id="@+id/llVasthunTips"
                            android:layout_weight="1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <com.google.android.material.card.MaterialCardView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                app:cardCornerRadius="14dp"
                                app:cardElevation="0dp">

                                <ImageView
                                    android:layout_width="70dp"
                                    android:layout_height="70dp"
                                    android:layout_gravity="center"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_12" />

                            </com.google.android.material.card.MaterialCardView>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:fontFamily="@font/ramabhadra"
                                android:maxLines="2"
                                android:text="VasthunTips"
                                android:textAlignment="center"
                                android:textColor="@color/black"
                                android:textSize="15sp" />
                        </LinearLayout>





                    </LinearLayout>


                </com.google.android.material.card.MaterialCardView>


            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="@dimen/_10sdp"
                android:layout_marginTop="@dimen/_15sdp"
                android:orientation="vertical">


                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:fontFamily="@font/ramabhadra"
                    android:text="భక్తి గీతాలు"
                    android:textColor="@color/calHeaderT"
                    android:textSize="@dimen/_16sdp" />


                <com.google.android.material.card.MaterialCardView
                    app:cardUseCompatPadding="true"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_120sdp"
                    app:cardBackgroundColor="@color/white"
                    app:cardCornerRadius="@dimen/_10sdp"
                    app:cardMaxElevation="@dimen/_2sdp">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:padding="@dimen/_5sdp">


                        <androidx.recyclerview.widget.RecyclerView
                            android:layout_centerVertical="true"
                            android:id="@+id/rcAudio"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content" />


                    </RelativeLayout>


                </com.google.android.material.card.MaterialCardView>


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="@dimen/_15sdp"
                android:layout_marginTop="@dimen/_15sdp"
                android:orientation="vertical">


                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:fontFamily="@font/ramabhadra"
                    android:text="దేవాలయాల సమాచారం"
                    android:textColor="@color/calHeaderT"
                    android:textSize="@dimen/_16sdp" />

                <com.google.android.material.card.MaterialCardView
                    app:cardUseCompatPadding="true"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_120sdp"
                    app:cardBackgroundColor="@color/white"
                    app:cardCornerRadius="@dimen/_10sdp"
                    app:cardMaxElevation="@dimen/_2sdp">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:padding="@dimen/_5sdp">


                        <androidx.recyclerview.widget.RecyclerView
                            android:layout_centerVertical="true"
                            android:id="@+id/rcTemple"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content" />


                    </RelativeLayout>


                </com.google.android.material.card.MaterialCardView>


            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="@dimen/_15sdp"
                android:layout_marginTop="@dimen/_15sdp"
                android:orientation="vertical">


                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:fontFamily="@font/ramabhadra"
                    android:text="నవగ్రహ - రుద్రాక్ష - నవరత్నాలు"
                    android:textColor="@color/calHeaderT"
                    android:textSize="@dimen/_16sdp" />

                <com.google.android.material.card.MaterialCardView
                    app:cardUseCompatPadding="true"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_100sdp"
                    app:cardBackgroundColor="@color/white"
                    app:cardCornerRadius="@dimen/_10sdp"
                    app:cardMaxElevation="@dimen/_2sdp">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:padding="@dimen/_5sdp">

                        <LinearLayout
                            android:weightSum="4"
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">



                            <com.google.android.material.card.MaterialCardView
                                android:id="@+id/cvGrahanalu"
                                app:cardUseCompatPadding="true"
                                app:cardElevation="0dp"
                                android:elevation="0dp"
                                android:layout_weight="1"
                                app:cardBackgroundColor="@color/white"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">


                                <ImageView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:background="@drawable/grahanalu_img"
                                    />





                            </com.google.android.material.card.MaterialCardView>
                            <com.google.android.material.card.MaterialCardView
                                android:id="@+id/cvNavagrahalu"
                                app:cardUseCompatPadding="true"
                                app:cardElevation="0dp"
                                android:elevation="0dp"
                                android:layout_weight="1"
                                app:cardBackgroundColor="@color/white"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">


                                <ImageView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:background="@drawable/navagrahalu_img"
                                    />





                            </com.google.android.material.card.MaterialCardView>
                            <com.google.android.material.card.MaterialCardView
                                android:id="@+id/cvNavarathnalu"
                                app:cardUseCompatPadding="true"
                                app:cardElevation="0dp"
                                android:elevation="0dp"
                                android:layout_weight="1"
                                app:cardBackgroundColor="@color/white"

                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">


                                <ImageView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:background="@drawable/navarathnalu_img"
                                    />





                            </com.google.android.material.card.MaterialCardView>
                            <com.google.android.material.card.MaterialCardView
